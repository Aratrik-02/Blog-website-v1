<%- include('partials/header'); -%>
<h1>Home</h1>
<p> <%= startingContent %></p>
<% something.forEach(element => { %>
    <h1> <%= element.title %></h1>
    <% if(element.body.length>=100){ %>
    <p> <%= element.body.slice(0,100)+"..."%>
        <a href="/post/<%= element.title %>">Read More</a>
    </p>
    <% } else { %>
    <p> <%= element.body %>
        <a href="/post/<%= element.title %>">Read More</a>
    </p>
    <% } %>
<% }) %>
<div>
    <a class ="comp" href ="/compose">Write</a>
</div>
<%- include('partials/footer'); -%>